{
    "name": "anomaly/streams-platform",
    "description": "Streams platform core library.",
    "authors": [
        {
            "name": "AnomalyLabs",
            "email": "projects@anomaly.is"
        }
    ],
    "require": {
        "laravel/framework": "5.1.13",
        "laravel/envoy": "1.0.*",
        "laravelcollective/html": "5.1.*",
        "doctrine/dbal": "2.5.*",
        "intervention/image": "2.1.*",
        "twig/twig": "1.18.*",
        "rcrowe/twigbridge": "0.7.*",
        "barryvdh/laravel-debugbar": "2.0.*",
        "coffeescript/coffeescript": "1.3.1",
        "michelf/php-markdown": "1.4.1",
        "aptoma/twig-markdown": "1.1.*",
        "guzzlehttp/guzzle": "6.0.*",
        "guzzlehttp/psr7": "1.1.*",
        "guzzlehttp/promises": "1.0.*",
        "kriswallsmith/assetic": "1.2.*",
        "leafo/scssphp": "0.1.0 ",
        "lmammino/jsmin4assetic": "1.0.*",
        "natxet/CssMin": "3.0.*",
        "oyejorge/less.php": "1.7.*",
        "ptachoire/cssembed": "1.0.*",
        "phpseclib/phpseclib": "0.3.*",
        "robclancy/presenter": "1.3.*",
        "mobiledetect/mobiledetectlib": "2.8.*",
        "nicmart/string-template": "0.1.*",
        "barryvdh/laravel-httpcache": "0.2.*",
        "phive/twig-extensions-deferred": "0.3.*",
        "symfony/yaml": "~2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "benconstable/phpspec-laravel": "~1.0",
        "henrikbjorn/phpspec-code-coverage": "~0.2"
    },
    "autoload": {
        "psr-4": {
            "Anomaly\\Streams\\Platform\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
